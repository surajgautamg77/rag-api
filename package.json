{
  "name": "rag-chatbot",
  "version": "1.0.0",
  "description": "RAG chatbot with document upload, embedding, and chat functionality",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "frontend": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "frontend:install": "cd frontend && npm install",
    "full:install": "npm install && npm run frontend:install",
    "full:dev": "concurrently \"npm run dev\" \"npm run frontend\"",
    "reprocess": "node reprocess-documents.js"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.3",
    "pgvector": "^0.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "rag",
    "chatbot",
    "openai",
    "postgresql",
    "vector"
  ],
  "author": "",
  "license": "MIT"
}
